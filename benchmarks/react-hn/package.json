        ]
      }
    }
  },
  "devDependencies": {
    "@babel/core": "^7.6.2",
    "@babel/preset-env": "^7.6.2",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-stage-0": "^7.0.0",
    "@parcel/config-default": "^2.0.0-alpha.1.1",
    "parcel": "^2.0.0-alpha.1.1"
  },
  "dependencies": {
    "events": "1.1.1",
    "firebase": "3.4.1",
    "history": "2.1.2",
    "isomorphic-fetch": "2.2.1",
    "react": "15.3.2",
    "react-dom": "15.3.2",
    "react-router": "2.8.1",
    "react-router-scroll": "0.3.2",
    "react-timeago": "3.1.3",
    "reactfire": "1.0.0",
    "setimmediate": "1.0.5",
    "url-parse": "1.1.1"
